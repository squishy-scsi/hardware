(rule "f.cu-bus-clearence"
  (layer "F.Cu")
  (condition "A.NetClass == 'SCSI_PHY' && A.Type != 'Via' && B.NetName == 'GND'")
  (constraint clearance (min 0.2mm))
)

(rule "b.cu-bus-clearence"
  (layer "B.Cu")
  (condition "A.NetClass == 'SCSI_PHY' && A.Type != 'Via' && B.NetName == 'GND'")
  (constraint clearance (min 0.2mm))
)
